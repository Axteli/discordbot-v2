{
    "env": {
        "browser": true,
        "node":true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "settings": {
        "SwitchCase":"4"
    },
    "rules": {
        "indent": [
            "error",
            "tab", 
            {"SwitchCase":1}
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
