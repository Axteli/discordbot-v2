{
    "name": "Discordbot-v2",
    "repository": "https://github.com/Axteli/discordbot-v2",
    "buildpacks": [
      {
        "url": "heroku/nodejs"
      }
    ],
    "env": {
      "OWNER_ID": {
        "description": "The id of the owner of the bot.",
        "value":"!"
      },
      "DISCORD_TOKEN": {
        "description": "The Discord Bot Token (https://discord.com/developers/applications)",
        "required": "true"
      },
        "SUPPORT_SERVER": {
          "description": "The main server of the bot.",
          "required":"true"
      },
      "EMBED_COLOR": {
        "description":"The color of all embeds of the bot.",
        "value":"36393F"
      },
      "MONGO_DB": {
        "description": "The link to the mongDB database.",
        "required":"true"
      }
  }
}